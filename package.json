{
  "name": "quiettime_coordinator_frontend",
  "version": "1.0.0",
  "description": "React frontend for the quiettime coordinator app",
  "main": "index.js",
  "watch": {
    "test": {
      "patterns": [
        "src",
        "tests"
      ],
      "extensions": "js,jsx",
      "quiet": true
    }
  },
  "scripts": {
    "test": "tape -r babel-register -r ignore-styles tests/**/*.js | faucet || true",
    "test-watch": "npm-watch",
    "build": "NODE_ENV=production webpack -p",
    "webpack-server": "API_URL=http://localhost:9000 webpack-dev-server -d --inline --hot",
    "web-server": "http-server -p 3000 .",
    "start": "npm run webpack-server & npm run web-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PortableStick/quiettime_coordinator_frontend.git"
  },
  "author": "Gregory Smith",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PortableStick/quiettime_coordinator_frontend/issues"
  },
  "homepage": "https://github.com/PortableStick/quiettime_coordinator_frontend#readme",
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators": "^6.13.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.6.0",
    "faucet": "0.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "http-server": "^0.9.0",
    "ignore-styles": "^5.0.1",
    "img-loader": "^1.3.1",
    "jsdom": "^9.9.1",
    "node-sass": "^4.1.1",
    "npm-watch": "^0.1.7",
    "postcss-loader": "^1.2.1",
    "react-addons-test-utils": "^15.4.1",
    "react-hot-loader": "^1.3.1",
    "sass-loader": "^4.1.1",
    "sinon": "^1.17.6",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "svgo": "^0.7.1",
    "svgo-loader": "^1.1.2",
    "tape": "^4.6.3",
    "tape-catch": "^1.0.6",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  }
}
